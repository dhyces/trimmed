{
    "schemaVersion": 1,
    "id": "trimmed",
    "version": "${version}",
  
    "name": "Trimmed",
    "description": "Better item overrides! Override it all!",
    "authors": [
      "Dhyces"
    ],
    "contact": {
      "homepage": "https://modrinth.com/mod/trimmed",
      "sources": "https://github.com/dhyces/trimmed/"
    },
  
